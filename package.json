{
	"name": "aspect-quiz-analyzer",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest watch",
		"test:integration": "playwright test --reporter=dot",
		"test:integration:ui": "playwright test --ui",
		"test": "npm run test:unit && npm run test:integration",
		"lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ."
	},
	"dependencies": {
		"@fontsource-variable/encode-sans": "^5.0.13",
		"@fontsource-variable/saira": "^5.0.29",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^8.57.0",
		"@playwright/test": "^1.45.1",
		"@sveltejs/adapter-vercel": "^5.4.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-shape": "^3.1.6",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.14.10",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-playwright": "^1.6.2",
		"eslint-plugin-svelte": "^2.41.0",
		"globals": "^13.24.0",
		"postcss": "^8.4.39",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.15.0",
		"vite": "^5.3.3",
		"vitest": "^2.0.0"
	},
	"type": "module"
}

{
	"name": "aspect-quiz-analyzer",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest watch",
		"test:integration": "playwright test --reporter=dot",
		"test:integration:ui": "playwright test --ui",
		"test": "npm run test:unit && npm run test:integration",
		"lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ."
	},
	"dependencies": {
		"@fontsource-variable/encode-sans": "^5.0.14",
		"@fontsource-variable/saira": "^5.0.30",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@playwright/test": "^1.46.1",
		"@sveltejs/adapter-vercel": "^5.4.3",
		"@sveltejs/kit": "^2.5.25",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-shape": "^3.1.6",
		"@types/eslint": "^9.6.1",
		"@types/node": "^20.16.2",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-playwright": "^1.6.2",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "^4.2.19",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.10",
		"tslib": "^2.7.0",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.3.0",
		"vite": "^5.4.2",
		"vitest": "^2.0.5"
	},
	"type": "module"
}

{
	"name": "aspect-quiz-analyzer",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest watch",
		"test:integration": "playwright test --reporter=dot",
		"test:integration:ui": "playwright test --ui",
		"test": "npm run test:unit && npm run test:integration",
		"lint": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ."
	},
	"dependencies": {
		"@fontsource-variable/encode-sans": "^5.1.1",
		"@fontsource-variable/saira": "^5.1.1",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@playwright/test": "^1.50.0",
		"@sveltejs/adapter-vercel": "^5.5.3",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/d3-scale": "^4.0.8",
		"@types/d3-shape": "^3.1.7",
		"@types/eslint": "^9.6.1",
		"@types/node": "^20.17.16",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-playwright": "^2.2.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.19.2",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.21.0",
		"vite": "^5.4.14",
		"vitest": "^2.1.8"
	},
	"overrides": {
		"@sveltejs/kit": {
			"cookie": "^0.7.0"
		}
	},
	"type": "module"
}
